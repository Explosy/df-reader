{
  "name": "df-reader",
  "type": "module",
  "version": "0.0.0-development",
  "description": "Library for reading E,B,G-df formats",
  "author": "Explosy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Explosy/df-reader.git"
  },
  "bugs": "https://github.com/Explosy/df-reader/issues",
  "keywords": [
    "EDF Reader",
    "BDF Reader",
    "GDF Reader"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.mjs",
      "require": "./index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "tsup src/index.ts --watch",
    "build": "tsup src/index.ts --minify --format cjs,esm --dts",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.1.1",
    "@changesets/cli": "^2.27.1",
    "eslint": "^8.54.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^0.34.6"
  }
}
